{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "test.html",
      "use": "@vercel/static"
    },
    {
      "src": "main.js",
      "use": "@vercel/static"
    },
    {
      "src": "config.js",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "/test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
