{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/test.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/main.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/config.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/test",
      "dest": "/public/test.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
